{
    actions: {
        get:{
            '/': 'Index',
            '/index': 'Index',
            '/manager': 'Manage',
            '/client': 'ClientAction',
            '/channel': 'ChannelAction',
            '/user': 'UserAction'
        },
        post:{
            '/api': 'Api'
        },
        request:{
            '/oauth2/authorize': 'Authorize',
            '/oauth2/token': 'Token',
            '/login': 'Login',
            '/api': 'Api',
            
            '/ta': 'TokenAction',
            '/ca': 'CodeAction'
        },
        regexp: [{
            'reg':/^\/\u\/([1-9]{1,}[0-9]{0,})$/,
            'ref':'UserAction'
        },{
            'reg':/^\/c\/([1-9]{1,}[0-9]{0,})$/,
            'ref':'ChannelAction'
        },{
            'reg':/^\/app\/([1-9]{1,}[0-9]{0,})$/,
            'ref':'ClientAction'
        }]
    }
}
