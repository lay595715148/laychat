{
    actions: {
        get:{
            '/': 'Index',
            '/index': 'Index',
            '/manager': 'Manage',
            '/client': 'ClientAction',
            '/channel': 'ChannelAction',
            '/user': 'UserAction'
        },
        post:{
            '/api': 'Api'
        },
        request:{
            '/oauth2/authorize': 'Authorize',
            '/oauth2/token': 'Token',
            '/login': 'Login',
            '/logout': 'Logout',
            '/api': 'Api',
            
            '/admin/token': 'TokenAction',
            '/admin/code': 'CodeAction'
        },
        regexp: [{
            'reg':/^\/\u\/(\d+)$/,
            'ref':'UserAction'
        },{
            'reg':/^\/c\/(\d+)$/,
            'ref':'ChannelAction'
        },{
            'reg':/^\/app\/(\d+)$/,
            'ref':'ClientAction'
        }]
    }
}
