{
    actions: {
        get:{
            '/': 'web.Index',
            '/index': 'web.Index',
            '/manager': 'Manage',
            '/client': 'web.ClientAction',
            '/channel': 'web.ChannelAction',
            '/user': 'web.UserAction'
        },
        post:{
            '/api': 'api.Api'
        },
        request:{
            '/oauth2/authorize': 'auth.Authorize',
            '/oauth2/token': 'auth.Token',
            '/login': 'auth.Login',
            '/logout': 'auth.Logout',
            '/redirect': 'auth.Redirect',
            
            '/api': 'api.Api',
            
            '/admin/token': 'admin.TokenAction',
            '/admin/code': 'admin.CodeAction'
        },
        regexp: [{
            'reg':/^\/\u\/(\d+)$/,
            'ref':'web.UserAction'
        },{
            'reg':/^\/c\/(\d+)$/,
